<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://glade.gnome.org/glade-2.0.dtd" >

<glade-interface>
  <widget class="GtkMenu" id="menu">
    <property name="visible">yes</property>

    <child>
      <widget class="GtkMenuItem" id="copy_fullpnlist">
        <property name="label" translatable="yes">Copy FullPropertyNameList</property>
        <property name="visible">yes</property>

        <signal name="activate" handler="copy_fullpnlist" />

        <accelerator key="c" modifiers="GDK_CONTROL_MASK" signal="activate" />
      </widget>
    </child>

    <child>
      <widget class="GtkMenuItem" id="paste_fullpnlist">
        <property name="label" translatable="yes">Paste FullPropertyNameList</property>
        <property name="visible">yes</property>

        <signal name="activate" handler="paste_fullpnlist" />

        <accelerator key="v" modifiers="GDK_CONTROL_MASK" signal="activate" />
      </widget>
    </child>

    <child>
      <widget class="GtkMenuItem" id="add_fullpnlist">
        <property name="label" translatable="yes">Add FullPN to FullPNList</property>
        <property name="visible">yes</property>

        <signal name="activate" handler="add_fullpnlist" />

        <accelerator key="v" modifiers="GDK_CONTROL_MASK | GDK_SHIFT_MASK" signal="activate" />
      </widget>
    </child>
  </widget>
</glade-interface>
